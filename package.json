{
  "name": "upis-api",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "Giedrius Bla≈æulionis <giedrius.bla@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "npm run build:prod && npm run start:prod",
    "start:dev": "nodemon",
    "start:prod": "cross-env NODE_PATH=build NODE_ENV=production node -r ./dotenv.js ./build/index.js",
    "build:prod": "npm i typescript copyfiles && npm run bundle",
    "build:prod:local": "npm run clean && npm run bundle",
    "bundle": "npm run compile && npm run static",
    "compile": "tsc",
    "static": "copyfiles -u 1 \"src/emails/templates/*.mjml\" \"src/translations/*.json\" \"build\"",
    "clean": "rimraf ./build",
    "browserslist": "npx browserslist",
    "outdated": "npx npm-check-updates",
    "lint": "npm run lint:code && npm run lint:format && npm run lint:filenames && npm run compile -- --noEmit",
    "lint:fix": "npm run lint:code:fix && npm run lint:format:fix",
    "lint:code": "npx eslint src --ext .ts,.tsx --report-unused-disable-directives",
    "lint:code:fix": "npm run lint:code -- --fix",
    "lint:filenames": "npx @ls-lint/ls-lint",
    "lint:format": "npx prettier --check \"src/**\"",
    "lint:format:fix": "npx prettier --write \"src/**\""
  },
  "dependencies": {
    "aws-sdk": "^2.1116.0",
    "cloudinary": "^1.34.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.14.2",
    "google-auth-library": "^8.7.0",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.1",
    "html-to-text": "^8.2.0",
    "http-graceful-shutdown": "^3.1.13",
    "i18next": "^21.8.16",
    "i18next-fs-backend": "^1.1.4",
    "i18next-http-middleware": "^3.2.1",
    "jsonwebtoken": "^9.0.0",
    "mjml": "^4.12.0",
    "mongoose": "^6.8.4",
    "mongoose-paginate-v2": "^1.7.1",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.7.5",
    "slugify": "^1.6.5",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "^1.11.2",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/html-to-text": "^8.1.0",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mjml": "^4.7.0",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.11",
    "@types/node": "^18.15.11",
    "@types/node-cron": "^3.0.7",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
