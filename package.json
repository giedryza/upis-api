{
  "name": "upis-api",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "Giedrius Bla≈æulionis <giedrius.bla@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "npm run build && npm run start:prod",
    "start:dev": "nodemon",
    "start:prod": "cross-env NODE_PATH=build NODE_ENV=production node -r ./dotenv.js ./build/index.js",
    "build": "npm run clean && npm run compile",
    "compile": "tsc",
    "clean": "rimraf ./build",
    "browserslist": "npx browserslist",
    "update": "npx npm-check-updates",
    "lint": "npm run lint:code && npm run lint:format && npm run lint:filenames && npm run compile -- --noEmit",
    "lint:fix": "npm run lint:code:fix && npm run lint:format:fix",
    "lint:code": "npx eslint src --ext .ts,.tsx",
    "lint:code:fix": "npm run lint:code -- --fix",
    "lint:filenames": "npx @ls-lint/ls-lint",
    "lint:format": "npx prettier --check \"src/**\"",
    "lint:format:fix": "npx prettier --write \"src/**\""
  },
  "dependencies": {
    "aws-sdk": "^2.1116.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.4.0",
    "express-validator": "^6.14.2",
    "handlebars": "^4.7.7",
    "helmet": "^5.1.0",
    "html-to-text": "^8.2.0",
    "http-graceful-shutdown": "^3.1.7",
    "i18n": "^0.15.0",
    "jsonwebtoken": "^8.5.1",
    "mjml": "^4.12.0",
    "mongoose": "^6.5.1",
    "mongoose-paginate-v2": "^1.7.0",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "nodemailer": "^6.7.5",
    "slugify": "^1.6.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "^1.11.2",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/html-to-text": "^8.1.0",
    "@types/i18n": "^0.13.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mjml": "^4.7.0",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.11",
    "@types/node": "^17.0.40",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}
