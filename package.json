  "license": "MIT",
  "engines": {
    "node": "12.18.4"
  },
  "scripts": {
    "build": "yarn clean && tsc",
    "start": "yarn build && yarn start:prod",
    "start:dev": "nodemon",
    "start:prod": "cross-env NODE_PATH=build NODE_ENV=production node -r ./dotenv.js ./build/index.js",
    "clean": "rimraf ./build",
    "lint": "eslint \"src/**\"",
    "lint:fix": "yarn lint --fix",
    "prettier:fix": "prettier --write \"src/**\""
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.7",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
